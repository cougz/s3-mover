# S3 Configuration
S3_ENDPOINT=https://s3.amazonaws.com
S3_ACCESS_KEY=your-access-key
S3_SECRET_KEY=your-secret-key
S3_BUCKET=my-bucket
S3_DESTINATION=

# File Selection
SOURCE_PATH=/data
FILE_PATTERN=*

# Upload Behavior
DELETE_AFTER_UPLOAD=true
MODE=watch
MOVE_SUBDIRS=false

# Cron Mode (only used when MODE=cron)
CRON_SCHEDULE=*/5 * * * *

# MinIO Client Extra Flags
MC_EXTRA_ARGS=
